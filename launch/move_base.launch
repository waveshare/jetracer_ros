<?xml version="1.0"?>
<launch>
  <node pkg="move_base" type="move_base" respawn="true" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find jetracer)/config/ackman/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find jetracer)/config/ackman/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find jetracer)/config/ackman/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find jetracer)/config/ackman/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find jetracer)/config/ackman/teb_local_planner_params.yaml" command="load" />
    
    <!-- <rosparam file="$(find jetracer)/ackman/diff/base_global_planner_param.yaml" command="load" /> -->
    <!-- <rosparam file="$(find jetracer)/config/ackman/move_base_params.yaml" command="load" /> -->
    
    <param name="base_global_planner" value="global_planner/GlobalPlanner" />
		<param name="planner_frequency" value="1.0" />
		<param name="planner_patience" value="5.0" />

		<param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
		<param name="controller_frequency" value="5.0" />
		<param name="controller_patience" value="15.0" />
		<param name="recovery_behavior_enabled" value="true"/>

    <param name="clearing_rotation_allowed" value="false" /> <!-- Our carlike robot is not able to rotate in place -->
  </node>
  
</launch>